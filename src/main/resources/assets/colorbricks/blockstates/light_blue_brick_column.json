{
    "multipart": [
        {
            "apply": {
                "model": "colorbricks:block/light_blue_brick_column_center"
            }
        },
        {
            "when": {
                "down": true
            },
            "apply": {
                "model": "colorbricks:block/light_blue_brick_column_end"
            }
        },
        {
            "when": {
                "up": true
            },
            "apply": {
                "model": "colorbricks:block/light_blue_brick_column_end",
                "x": 180,
                "uvlock": true
            }
        }
    ]
}